{
  "name": "pawstagram",
  "version": "1.0.0",
  "description": "Social media platform for pets with adoption features",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000",
    "build": "npm run build-css",
    "build-css": "tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch",
    "build-css-prod": "tailwindcss -i ./src/css/input.css -o ./src/css/output.css --minify"
  },
  "keywords": ["pets", "social-media", "adoption", "tailwindcss", "supabase"],
  "author": "SoyJadee",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "live-server": "^1.2.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0"
  }
}